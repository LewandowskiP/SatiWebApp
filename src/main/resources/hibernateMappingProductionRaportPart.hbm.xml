<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="HibernateClasses">

    <class name="ProductionRaportPart" table="ProductionRaportPart">
        <id name="id" type="int" column="productionRaportPart_id">
            <generator class="identity"/>
        </id>
        <property name="labTestState" type="int" column="labTestState"/>
        <many-to-one name="productType" class="ProductType" column="productType_id"/>
        <many-to-one name="emp" class="Employee" column="employee_id"/>
    </class>

</hibernate-mapping>